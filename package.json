{
  "name": "electron-vite-vue3",
  "private": true,
  "version": "0.0.0",
  "description": "electron vite vue3 template",
  "keywords": [
    "electron",
    "vite",
    "vue3",
    "vue",
    "vue devtools",
    "electron-vue",
    "electron-vite"
  ],
  "main": "electron/index.js",
  "scripts": {
    "start": "concurrently -k 'npm run dev' 'npm run electron'",
    "release": "npm run build && electron-builder",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "wait-on tcp:3000 && cross-env NODE_ENV=development electron ."
  },
  "dependencies": {
    "date-fns": "^3.6.0",
    "electron-dl": "^4.0.0",
    "electron-log": "^5.1.5",
    "element-plus": "^2.7.5",
    "resolve": "^1.22.8",
    "sqlite3": "^5.1.7",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.2.0",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "electron": "^17.2.0",
    "electron-builder": "^22.14.13",
    "electron-reloader": "^1.2.3",
    "sass": "^1.49.9",
    "vite": "^2.8.0",
    "wait-on": "^6.0.1"
  }
}
